{
  "name": "obsidian-simple-icons",
  "version": "0.1.0",
  "description": "Associate Lucide icons to markdown files throughout Obsidian UI",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "type-check": "tsc -noEmit -skipLibCheck",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "npx prettier --write \"src/**/*.{ts,css}\"",
    "format:check": "npx prettier --check \"src/**/*.{ts,css}\"",
    "pre-commit:check": "bash scripts/pre-commit-check.sh",
    "ci:check": "npm run lint && npx prettier --check \"src/**/*.{ts,css}\" && npm run type-check && npm run build",
    "version": "node version-bump.mjs && git add src/manifest.json versions.json",
    "release": "npm run build && npm run release:tag",
    "release:tag": "node -e \"console.log('v' + require('./package.json').version)\" | xargs git tag",
    "prepare": "husky install || true"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "lucide",
    "icons"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "dotenv": "^16.0.3",
    "esbuild": "0.17.3",
    "glob": "^11.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "obsidian": "latest",
    "prettier": "^3.1.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
