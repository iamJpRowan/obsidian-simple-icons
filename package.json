{
  "name": "obsidian-lucide-icons",
  "version": "0.1.0",
  "description": "Associate Lucide icons to markdown files throughout Obsidian UI",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "lucide",
    "icons"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "lucide-static": "^0.460.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "dotenv": "^16.0.3",
    "esbuild": "0.17.3",
    "glob": "^11.0.1",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
}
